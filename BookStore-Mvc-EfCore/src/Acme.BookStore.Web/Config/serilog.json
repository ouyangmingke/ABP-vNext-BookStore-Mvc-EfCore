{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Expressions" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.AspNetCore": "Error",
        "Microsoft.AspNetCore.Cors.Infrastructure.CorsService": "Error",
        "Volo.Abp": "Warning",
        "Microsoft.AspNetCore.Hosting": "Error"
      }
    },
    "WriteTo:Debug": {
      "Name": "Logger",
      "Args": {
        "configureLogger": {
          "Filter": [
            {
              "Name": "ByIncludingOnly",
              "Args": {
                "expression": "@l = 'Debug'"
              }
            }
          ],
          "WriteTo": [
            {
              "Name": "Async",
              "Args": {
                "configure": [
                  {
                    "Name": "File",
                    "Args": {
                      "path": "Logs/Debug-.txt",
                      "rollingInterval": "Day",
                      "retainedFileCountLimit": 7
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    },
    "WriteTo:Information": {
